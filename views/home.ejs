<%- include("partials/header")-%>
    <link rel="stylesheet" href="css/style.css">
    <title>Conference Management Portal</title>
    </head>

    <body>
        <div class="background">
            <img class="header-image" src="/images/logo_img.png" alt="">
            <div class="header">
                <p class="heading">Conference Management Portal</p>
            </div>
            <div class="container">
                <img class="login-icon-image" src="/images/loginIcon.png" alt="">
                <p class="form_heading">ACCOUNT LOGIN</p>
                <form action="/" method="POST" class="login-email">
                    <div class="input-group">
                        <input type="text" placeholder="EMAIL ID" name="username" value="" required>
                    </div>
                    <div class="input-group">
                        <input type="password" placeholder="PASSWORD" name="password" value="" required>
                    </div>
                    <div class="input-group">
                        <button name="LOG IN" class="btn">LOG IN</button>
                    </div>
                    <p class="login-register-text">Didn't register for any conference? <a
                            href="/allconferences"><u>Register Now</u></a>.</p>
                </form>
            </div>
            <%- include("partials/footer")-%>
    </body>

    </html>